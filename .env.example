# Example .env file for Agent Service approach
# Copy this to .env and fill in your values

# Azure AI Foundry Project Endpoint
# Find this in AI Foundry portal > Your Project > Overview
PROJECT_ENDPOINT=https://your-project.services.ai.azure.com/api/projects/your-project-id

# Playwright Connection Name
# Created in AI Foundry portal > Management Center > Connected Resources
AZURE_PLAYWRIGHT_CONNECTION_NAME=playwright-connection

# Model Deployment Name
# Your GPT-4 deployment name from AI Foundry portal > Models + Endpoints
MODEL_DEPLOYMENT_NAME=gpt-4.1

# Enable tracing content recording (optional, default: true)
AZURE_TRACING_GEN_AI_CONTENT_RECORDING_ENABLED=true
